   ],
   "source": [
    "rows = 5\n",
    "for i in range(rows,-1,-1):\n",
    "    for j in range(i + 1, rows):\n",
    "        print('*', '', end='')\n",
    "    for l in range(i):\n",
    "        print('    ', end='')\n",
    "    for k in range(rows - 1, i, -1):\n",
    "        print('*', '',end='')\n",
    "    print('\\n')"
   ]
